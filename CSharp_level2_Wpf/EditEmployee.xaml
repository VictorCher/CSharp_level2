<Window x:Class="CSharp_level2_Wpf.EditEmployee"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharp_level2_Wpf"
        mc:Ignorable="d"
        Title="Редактирование" Height="100" Width="330" ShowInTaskbar="False" ResizeMode="NoResize">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="165,40,0,0" VerticalAlignment="Top" Width="150" SelectionChanged="comboBox_SelectionChanged"/>
        <Label x:Name="label" Content="Имя" HorizontalAlignment="Left" Height="30" Margin="10,10,0,0" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="label_Copy" Content="Отдел" HorizontalAlignment="Left" Height="30" Margin="165,10,-21,0" VerticalAlignment="Top" Width="150"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="22" Margin="10,40,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="150" IsEnabled="False"/>

    </Grid>
</Window>
